{
  "name": "phonebook",
  "version": "1.0.0",
  "description": "A simple and practical telephone book",
  "main": "index.js",
  "scripts": {
    "build": "echo 'hello build'",
    "dev": "nodemon index.js",
    "start": "node index.js",
    "migrations:generate": "sequelize-cli  migration:generate --name",
    "migrations:run": "sequelize-cli  db:migrate",
    "migrations:revert": "sequelize-cli  db:migrate:undo",
    "migrations:delete": "sequelize-cli  db:migrate:undo:all"
  },
  "keywords": [
    "phonebook",
    "javascript",
    "react",
    "express",
    "node"
  ],
  "author": "Marvin Berrio <marvinccp@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "sequelize-cli": "^6.4.1"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "create-react-app": "^5.0.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.2",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.3"
  }
}
